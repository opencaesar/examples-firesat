@dc:title "ADCS and GNC Assemblies"
description <http://opencaesar.io/examples/firesat/programs/earth-science/projects/firesat/workpackages/06/07/subsystems/ang/assemblies#> as ang-assemblies {

	uses <http://purl.org/dc/elements/1.1/> as dc

	uses <http://www.w3.org/2000/01/rdf-schema#> as rdfs

	uses <http://imce.jpl.nasa.gov/foundation/base#> as base

	uses <http://opencaesar.io/examples/firesat/disciplines/fse/fse#> as fse

	extends <http://opencaesar.io/examples/firesat/programs/earth-science/projects/firesat/workpackages/06/systems/spc/subsystems/ang/ang#> as ang

	// ADCS and GNC Subsystem
	
	ref instance ang:ADCSAndGNC [
		base:aggregates Magnetometer
		base:aggregates GPSAntenna
		base:aggregates GPSReceiverUnit
		base:aggregates ADCSElectronicsUnit
		base:aggregates SunSensor
		base:aggregates EarthSensor
		base:aggregates ReactionWheel1
		base:aggregates ReactionWheel2
		base:aggregates ReactionWheel3
		base:aggregates ReactionWheel4
		base:aggregates Magnetorquer1
		base:aggregates Magnetorquer2
		base:aggregates Magnetorquer3
	]

	// Magnetometer

	@rdfs:label "Magnetometer"
	instance Magnetometer : fse:Assembly [
		base:hasIdentifier "513000"
	]

	// GPS Antenna

	@rdfs:label "GPS Antenna"
	instance GPSAntenna : fse:Assembly [
		base:hasIdentifier "515000"
	]

	// GPS Receiver Unit

	@rdfs:label "GPS Receiver Unit"
	instance GPSReceiverUnit : fse:Assembly [
		base:hasIdentifier "516600"
	]

	// ADCS Electronics Unit

	@rdfs:label "ADCS Electronics Unit"
	instance ADCSElectronicsUnit : fse:Assembly [
		base:hasIdentifier "516800"
	]

	// Sun Sensor

	@rdfs:label "Sun Sensor"
	instance SunSensor : fse:Assembly [
		base:hasIdentifier "556000"
	]

	// Earth Sensor

	@rdfs:label "Earth Sensor"
	instance EarthSensor : fse:Assembly [
		base:hasIdentifier "557000"
	]

	// Reaction Wheel 1

	@rdfs:label "Reaction Wheel 1"
	instance ReactionWheel1 : fse:Assembly [
		base:hasIdentifier "514000-1"
	]

	// Reaction Wheel 2

	@rdfs:label "Reaction Wheel 2"
	instance ReactionWheel2 : fse:Assembly [
		base:hasIdentifier "514000-2"
	]

	// Reaction Wheel 3

	@rdfs:label "Reaction Wheel 3"
	instance ReactionWheel3 : fse:Assembly [
		base:hasIdentifier "514000-3"
	]

	// Reaction Wheel 4

	@rdfs:label "Reaction Wheel 4"
	instance ReactionWheel4 : fse:Assembly [
		base:hasIdentifier "514000-4"
	]

	// Magnetorquer 1

	@rdfs:label "Magnetorquer 1"
	instance Magnetorquer1 : fse:Assembly [
		base:hasIdentifier "516900-1"
	]

	// Magnetorquer 2

	@rdfs:label "Magnetorquer 2"
	instance Magnetorquer2 : fse:Assembly [
		base:hasIdentifier "516900-2"
	]

	// Magnetorquer 3

	@rdfs:label "Magnetorquer 3"
	instance Magnetorquer3 : fse:Assembly [
		base:hasIdentifier "516900-3"
	]

}